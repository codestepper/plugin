{
  "name": "codestepper",
  "description": "",
  "version": "1.0",
  "action": {
    "default_title": "Codestepper"
  },
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "storage", "activeTab", "scripting", "notifications"],
  "icons": {
    "16": "codestepper.png",
    "48": "codestepper.png",
    "128": "codestepper.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["*"],
      "matches": ["<all_urls>"]
    }
  ]
}
